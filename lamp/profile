
JAVA_CMD=java

LAMP_JVM=" -Dlamp.conf.dir=/etc/opsgenie/lamp $LAMP_JVM"
LAMP_JVM=" -Dlamp.scripts.dir=/var/opsgenie/lamp/scripts $LAMP_JVM"
LAMP_JVM=" -Dlamp.logs.dir=/var/log/opsgenie/lamp $LAMP_JVM"

LAMP_CLASSPATH=LAMP_CLASSPATH:/var/lib/opsgenie/lamp/*

LAMP_JVM="$LAMP_JVM -Xmx512m -Xms256m"

if test -n "$JRE_HOME"
then
   unset JRE_HOME
fi

umask 002
